<!DOCTYPE html>
<html>
<head>
  <title>Lease Requests</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
  <div class="container">
    <h1>Lease Requests</h1>
    <div id="leaseRequestsList"></div>
  </div>

  <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
  <script>
    $(document).ready(function() {
      // Fetch lease requests data from the server using AJAX
      $.ajax({
        url: '/api/leaserequests',
        method: 'GET',
        success: function(response) {
          // Handle the response and update the leaseRequestsList element
          var leaseRequestsList = $('#leaseRequestsList');
          // Iterate through the lease requests and append them to the leaseRequestsList element
          response.forEach(function(leaseRequest) {
            leaseRequestsList.append('<div class="card">' + leaseRequest.text + '</div>');
          });
        }
      });
    });
  </script>
</body>
</html>
